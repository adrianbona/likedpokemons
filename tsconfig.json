{
    "include": [
        "./src/**/*"
    ],
    "compilerOptions": {
        "target": "es2016",
        "module": "esnext",
        "jsx": "react",
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "lib": ["dom", "es2016"]
      }
}